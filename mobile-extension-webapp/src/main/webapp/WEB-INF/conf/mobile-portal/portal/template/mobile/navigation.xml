<node-navigation
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2 http://www.gatein.org/xml/ns/gatein_objects_1_2"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
  <priority>1</priority>
  <page-nodes>
    <node>
      <uri>overview</uri>
      <name>overview</name>
      <label>#{portal.mobile.overview}</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::overview</page-reference>
    </node>
    
    <node>
      <uri>news</uri>
      <name>news</name>
      <label>#{portal.mobile.news}</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::news</page-reference>
    </node>

    <node>
      <uri>products</uri>
      <name>products</name>
      <label>#{portal.mobile.products}</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::products</page-reference>
      <node>
	      <uri>products/eventing</uri>
	      <name>eventing</name>
	      <label>Eventing</label>
	      <visibility>DISPLAYED</visibility>
	      <page-reference>portal::@owner@::eventing</page-reference>
	   </node>
      <node>
	      <uri>products/gallery</uri>
	      <name>gallery</name>
	      <label>Gallery</label>
	      <visibility>DISPLAYED</visibility>
	      <page-reference>portal::@owner@::gallery</page-reference>
	   </node>
    </node>

    

    <node>
      <uri>contactus</uri>
      <name>contactus</name>
      <label>#{portal.mobile.contactus}</label>
      <page-reference>portal::@owner@::contactus</page-reference>
      <node>
        <uri>contactus/confirmation</uri>
        <name>confirmation</name>
        <label>#{portal.mobile.confirmation}</label>
        <visibility>HIDDEN</visibility>
        <page-reference>portal::@owner@::confirmation</page-reference>
      </node>
    </node>

    <node>
      <uri>detail</uri>
      <name>detail</name>
      <label>#{portal.mobile.detail}</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::detail</page-reference>
    </node>
	
	<node>
      <uri>catalog</uri>
      <name>catalog</name>
      <label>#{portal.mobile.catalog}</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::catalog</page-reference>
    </node>

    <node>
      <uri>printviewer</uri>
      <name>printviewer</name>
      <label>#{portal.mobile.printviewer}</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::printviewer</page-reference>
    </node>

    <node>
      <uri>searchResult</uri>
      <name>searchResult</name>
      <label>#{portal.mobile.searchResult}</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::@owner@::searchResult</page-reference>
    </node>

    <node>
      <uri>newAccount</uri>
      <name>newAccount</name>
      <label>#{portal.mobile.newAccount}</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::@owner@::newAccount</page-reference>
    </node>

  </page-nodes>
</node-navigation>
